      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-react": "^7.0.0",
    "commitizen": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "prettier": "^1.14.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2",
    "rollup": "^1.0.2",
    "rollup-plugin-babel": "^4.2.0",
