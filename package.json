  "version": "0.2.1",
  "main": "dist/unstyled.cjs.js",
  "module": "dist/unstyled.cjs.js",
  "author": "sarasate",
  "license": "MIT",
  "keywords": [
    "react",
    "css",
    "css-in-js",
    "styled-components",
    "unstyled"
  ],
  "homepage": "https://www.unstyled.io",
  "bugs": {
    "url": "https://github.com/sarasate/unstyled/issues"
  },
  "dependencies": {
