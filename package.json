    "typeface-lato": "^0.0.54"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "prebuild": "rimraf dist",
    "prepublish": "yarn build",
    "test": "echo \"Info: no test specified\" && exit 0",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release",
    "format": "eslint ./**/*.js --fix"
